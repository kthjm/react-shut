{
  "name": "react-slidelay",
  "version": "0.0.0",
  "description": "",
  "main": "dist/cjs.js",
  "jsnext:main": "dist/es.js",
  "module": "dist/es.js",
  "repository": "kthjm/react-slidelay",
  "author": "kthjm <toxictoxer@gmail.com>",
  "license": "MIT",
  "keywords": [],
  "nyc": {
    "require": [
      "babel-register",
      "jsdom-global/register"
    ],
    "include": [
      "src"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "scripts": {
    "//pretest": "flow",
    "test": "cross-env NODE_ENV=TEST nyc mocha",
    "//prebuild": "yarn test",
    "build": "cross-env NODE_ENV=BUILD node rollup.config.js",
    "postbuild": "run-s format copy",
    "format": "prettier --write **/*.js",
    "copy": "flow-copy-source --verbose ./src ./dist",
    "report": "nyc report --reporter=lcov",
    "codecov": "codecov",
    "coverage": "run-s report codecov"
  },
  "dependencies": {
    "atra": "^1.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-rewire": "^1.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.24.1",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "flow-bin": "^0.59.0",
    "flow-copy-source": "^1.2.1",
    "flow-typed": "^2.2.1",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^4.0.1",
    "npm-run-all": "^4.1.1",
    "nyc": "^11.1.0",
    "power-assert": "^1.4.4",
    "prettier": "^1.7.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "rewire": "^3.0.2",
    "rollup": "^0.52.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-flow": "^1.1.1",
    "sinon": "^4.0.1"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  }
}
